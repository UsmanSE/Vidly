@model Vidly.Models.Movie

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>ReleaseDate: @Model.ReleaseDate</li>
    <li>DateAdded: @Model.DateAdded</li>
    <li>Number Of Stock: @Model.Stock</li>
</ul>